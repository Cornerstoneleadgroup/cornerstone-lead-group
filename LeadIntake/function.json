{
  "scriptFile": "main.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [ "post", "options" ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "type": "blob",
      "direction": "out",
      "name": "outBlob",
      "path": "leads/{datetime:yyyy-MM-ddTHH-mm-ssZ}_{rand-guid}.json",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
